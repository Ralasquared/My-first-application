<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.drsebi">

    <application
        android:allowBackup="true"
        tools:replace="android:icon"
        android:icon="@drawable/sebi_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Game.Game" />
        <activity android:name=".Diet.Vegetables" />
        <activity android:name=".Diet.Spices_and_seasonings" />
        <activity android:name=".Diet.Oils" />
        <activity android:name=".Diet.Nuts_and_seeds" />
        <activity android:name=".Diet.Fruits" />
        <activity android:name=".Dietplan" />
        <activity android:name=".Herbs" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>